def selection_sort(list):
  for i in range (len(list)):
    min = list[i]
    adding_index = i+1
    for adding_index in range (len(list)):
      if list[adding_index] < min:
        min = list[adding_index]
  return list
        
list = [5,3,2,4,6]
selection_sort(list)
print(list)
